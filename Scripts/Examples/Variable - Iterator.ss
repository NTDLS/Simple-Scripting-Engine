<%

var iIncrement as Numeric = 10

Print("iIncrement: " & iIncrement)
iIncrement++
Print("iIncrement: " & iIncrement)
iIncrement--
Print("iIncrement: " & iIncrement)
iIncrement += (66.77 + 0.23) + 0.5 ;Will equal 77.5
Print("iIncrement: " & iIncrement)
iIncrement -= (66.77 + 0.23) + 0.5 ;Will equal 10
Print("iIncrement: " & iIncrement)

%>

